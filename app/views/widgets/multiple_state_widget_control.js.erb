<%if @widget_data[:widgetName] == "wizard_buttons"%>
	$("<%= escape_javascript( widget_replace_selector( @widget_data[:parentNode] ) )%>").replaceWith("<%= escape_javascript( render 'widgets/multiple_state/widget', locals: @widget_data[:parentNode] ) %>");
<%else%>
	$("<%= escape_javascript( widget_replace_selector( @widget_data ) )%>").replaceWith("<%= escape_javascript( render 'widgets/multiple_state/widget', locals: @widget_data ) %>");
<%end%>