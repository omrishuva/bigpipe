<% state = widget_state(locals) %>
<%if state == :owner_before_commit %>

	<div class=<%="#{locals[:elementName]}"%> id=<%=widget_id(locals)%>  >
		<%=render "widgets/ugc/number/owner_before_commit", locals: locals%>
	</div>

<%elsif state == :owner_after_commit %>

	<div class=<%="#{locals[:elementName]}"%> id=<%=widget_id(locals)%>  >
		<%=render "widgets/ugc/number/owner_after_commit", locals: locals%>
	</div>

<%elsif state == :user_before_commit %>

	<div class=<%="#{locals[:elementName]}"%> id=<%=widget_id(locals)%>  >
		<%=render "widgets/ugc/number/user", locals: locals%>
	</div>

<%elsif state == :user_after_commit %>

	<div class=<%="#{locals[:elementName]}"%> id=<%=widget_id(locals)%>  >
		<%=render "widgets/ugc/number/user", locals: locals%>	
	</div>
<%elsif state == :edit %>
	<div class=<%="#{locals[:elementName]}"%> id=<%=widget_id(locals)%>  >
		<%=render "widgets/ugc/number/edit", locals: locals%>
	</div>
<%end%>