<% inputId = generate_id%>
<input 
	id="<%=inputId%>" 
	class="controls pac-input" 
	data-location-input-id="<%="#{locals[:objectName]}_#{locals[:key]}"%>"
	data-place-type="<%=locals[:placeType]%>"
	data-submit-on-chnage="<%=locals[:submitOnChange]%>"
	data-place-id="<%=locals[:value]%>"
	data-key="<%=locals[:key]%>"
  type="text"
	placeholder="<%="#{I18n.t locals[:placeholder]}"%>" >

<%= render 'widgets/multiple_state/save_cancel_buttons', locals: locals.merge(inputId: inputId) %>
