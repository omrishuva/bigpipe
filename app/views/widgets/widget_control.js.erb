<%unless @widget_data[:saveOnly] == "true" %>
	<% render_path =  @widget_data[:wizard] == true ?  "widgets/wizard/wizard" : "widgets/widget" %>
	$("<%= escape_javascript( widget_replace_selector( @widget_data ) )%>").replaceWith("<%= escape_javascript( render render_path, locals: @widget_data ) %>");
<%end%>