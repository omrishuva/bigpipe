<%= render "widgets/ugc/text/multiple_select_box/multiple_select", 
		locals: { 
							id: input_id(locals), 
							selectOptions: locals[:selectOptions], 
							selectedValues: locals[:value], 
							maxSelections: locals[:maxSelections],
							placeholder: locals[:placeholder] 
						}
%>

<%=render "widgets/ugc/text/button", 
						locals: locals.merge( 
																	dataType: "StringArray",
																	class: "btn btn-success smallBtn", 
																	state: "save", 
																	button_text: "save" )
	%>

<%=render "widgets/ugc/text/button", 
						locals: locals.merge( 
																	class: "btn btn-danger smallBtn", 
																	state: "cancel", 
																	button_text: "cancel" )
	%>